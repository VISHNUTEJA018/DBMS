SET SERVEROUTPUT ON;
DECLARE
X EMP.ENAME%TYPE;
E1 EXCEPTION;
BEGIN
  SELECT EMP.ENAME INTO X
  FROM EMP
  WHERE EMPNO = 7369;
  IF X = 'SMITH' THEN
    RAISE E1;
  ELSE
    DBMS_OUTPUT.PUT_LINE(' EMP MAME : '||X);
  END IF;
EXCEPTION
  WHEN E1 THEN
    DBMS_OUTPUT.PUT_LINE('...SMITH IS IN EXCEPTION...');
END;
